; lexical verb complement, but not the other complements, if any.

arg-comp-aux := verb-lex & basic-two-arg &
  [ SYNSEM.LOCAL [ CAT [ HEAD.MOD < >,
			 VAL [ SUBJ < #subj >,
                             COMPS < #comps . #vcomps >,
                             SPEC < > ] ],
                   CONT.HOOK.XARG #xarg ],
    ARG-ST < #subj &
             [ LOCAL [ CONT.HOOK.INDEX #xarg,
                       CAT [ VAL [ SPR < >,
                                   COMPS < > ],
                             HEAD noun ] ] ],
             #comps &
             [ LOCAL [ CAT [ HEAD verb,
				VAL [ SUBJ < [ NON-LOCAL non-local-none ] >,
                                   COMPS #vcomps ],
                             HEAD verb ],
                       CONT.HOOK.XARG #xarg ] ] > ].



